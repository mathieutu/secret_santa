{
    "name": "secret-santa/find-matches",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "SecretSanta\\FindMatches\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SecretSanta\\FindMatches\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
